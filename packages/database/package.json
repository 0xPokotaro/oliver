{
  "name": "@oliver/database",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./generated/client": {
      "types": "./generated/client/index.d.ts",
      "default": "./generated/client/index.js"
    }
  },
  "scripts": {
    "prisma:gen": "prisma generate --schema src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --create-only --schema src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema src/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema src/prisma/schema.prisma && prisma db seed --schema src/prisma/schema.prisma",
    "prisma:seed": "tsx src/prisma/seed.ts",
    "postinstall": "prisma generate --schema src/prisma/schema.prisma",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop"
  },
  "prisma": {
    "seed": "tsx src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/client-runtime-utils": "^7.2.0",
    "pg": "^8.16.3",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "prisma": "^7.2.0",
    "tsx": "^4.19.2"
  }
}
