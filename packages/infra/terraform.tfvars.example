  X402_MAX_AMOUNT_REQUIRED = ""
  X402_NETWORK             = ""
  X402_MAX_TIMEOUT_SECONDS = ""
  X402_DESCRIPTION         = ""
  FACILITATOR_URL          = ""
}

api_name     = "api-dev"
cors_enabled = true

